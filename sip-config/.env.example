# Environment Variables for SIP Setup
# Copy this to .env and fill in your values

# ============================================
# LiveKit Server Configuration
# ============================================
LIVEKIT_URL=ws://localhost:7880
LIVEKIT_API_KEY=devkey
LIVEKIT_API_SECRET=secret

# ============================================
# SIP Server Configuration
# ============================================
# Your server's public IP (required for SIP)
SIP_PUBLIC_IP=YOUR_SERVER_PUBLIC_IP

# ============================================
# Twilio Configuration (if using Twilio)
# ============================================
TWILIO_ACCOUNT_SID=your_account_sid
TWILIO_AUTH_TOKEN=your_auth_token
TWILIO_PHONE_NUMBER=+1234567890
TWILIO_SIP_DOMAIN=your-domain.sip.twilio.com

# ============================================
# Telnyx Configuration (if using Telnyx)
# ============================================
TELNYX_API_KEY=your_api_key
TELNYX_SIP_USERNAME=your_sip_username
TELNYX_SIP_PASSWORD=your_sip_password
TELNYX_PHONE_NUMBER=+1234567890

# ============================================
# Agent Configuration (your existing keys)
# ============================================
OPENAI_API_KEY=your_openai_key
ASSEMBLYAI_API_KEY=your_assemblyai_key
CARTESIA_API_KEY=your_cartesia_key
